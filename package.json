{
  "name": "slidev-theme-fennecfoxsw-personal",
  "version": "0.0.1",
  "license": "MIT",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@codemirror/theme-one-dark": "^6.1.1",
    "@slidev/theme-default": "^0.21.2",
    "@slidev/types": "^0.40.3",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.4",
    "prism-themes": "^1.9.0",
    "theme-vitesse": "^0.6.2"
  },
  "devDependencies": {
    "@slidev/cli": "^0.40.3",
    "@types/node": "^18.15.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "slidev": {
    "colorSchema": "dark",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Noto Sans KR",
        "mono": "Space Mono"
      },
      "hightlighter": "prism"
    }
  }
}
